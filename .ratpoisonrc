#!/bin/sh

exec feh --bg-scale ~/walls/circuit_onedark_no_logo.png
exec /usr/bin/rpws init 9 -k
exec wmname LG3D
exec picom --experimental-backends

set winname title
set wingravity center
set transgravity center
set bargravity center
set border 2
set barborder 3
set barpadding 1 1
set font "Iosevka:pixelsize=12"
set bgcolor #222222
set fgcolor #f9f9f9
set bwcolor #222222
set fwcolor #458588


escape Super_L

bind t exec xterm
bind s-b exec qutebrowser
bind s-D exec rofi -show drun
bind s-x exec powermenu
bind s-S exec screenshot
bind s-Return exec alacritty
bind s-q delete
bind s-Q only
bind s-t time
bind s-h focusleft
bind s-j focusdown
bind s-k focusup
bind s-l focusright


bind b exec qutebrowser
